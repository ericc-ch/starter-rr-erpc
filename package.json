    "react-router": "^7.6.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.9.1",
    "@echristian/eslint-config": "^0.0.44",
    "@react-router/dev": "^7.6.3",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^20.19.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.30.1",
    "lint-staged": "^16.1.2",
    "playwright": "^1.53.2",
