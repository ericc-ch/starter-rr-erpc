    "test": "vitest",
    "typecheck": "bun run cf-typegen && react-router typegen && tsc -b"
  },
  "simple-git-hooks": {
    "pre-commit": "bunx lint-staged"
  },
  "lint-staged": {
    "*": "bun run lint --fix"
  },
  "dependencies": {
